[csv]
state = on # If not specified, this defaults to off
directory = /path/to/download/dir

[json]
state = on
directory = /path/to/download/dir

[kinesis]
state = off
keyid = AWS_ACCESS_KEY_ID
secretkey = AWS_SECRET_ACCESS_KEY
stream = kinesis-stream-name
region = us-east-1 # optional, defaults to us-east-1

[product "foo"]
key = API_KEY
secret = API_SECRET
token = API_TOKEN

[product "bar"]
key = API_KEY
secret = API_SECRET
token = API_TOKEN
